---
import ProjectHeader from "./ProjectHeader.astro";
import ProjectNatanya from "./ProjectNatanya.astro";
---

<section class="relative">
  <!-- mesh -->
  <img
    loading="lazy"
    src="/images/mesh.png"
    alt=""
    width="1400"
    height="1292"
    class="hidden sm:block absolute top-[-150px] sm:top-[-150px] sm:left-[450px] right-0 min-w-[554px] lg:min-w-[1400px]"
  />

  <ProjectHeader />

  <div class="sm:mt-36 mt-20 px-0 sm:px-12 md:px-16">
    <ProjectNatanya />
  </div>

  <div class="mt-36 relative">
    <img
      loading="lazy"
      src="/images/mesh.png"
      alt=""
      width="1400"
      height="1292"
      class="hidden sm:block absolute top-[-1000px] left-[-250px] sm:min-h-[1400px] w-auto aspect-auto"
    />
  </div>

  <div class="sm:mt-36 mt-20 px-0 sm:px-12 md:px-16">
    <ProjectNatanya />
  </div>

  <div class="mt-36"></div>
</section>
